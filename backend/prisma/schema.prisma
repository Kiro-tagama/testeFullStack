generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tools {
  id           Int       @id @default(autoincrement())
  name         String
  description  String
  status       String
  initial_date DateTime? @default(now())
  final_date   DateTime?
  user         String?
}
